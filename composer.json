{
    "name": "silverstripe/auditor",
    "description": "Adds security audit trail to SilverStripe.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "audit"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": "^8.1",
        "silverstripe/framework": "^6",
        "monolog/monolog": "^3.2",
        "psr/log": "^3",
        "silverstripe/silverstripe-proxy-db": "^3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3",
        "silverstripe/versioned": "^3",
        "silverstripe/standards": "^1",
        "phpstan/extension-installer": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\Auditor\\": "code/",
            "SilverStripe\\Auditor\\Tests\\": "tests/"
        }
    },
    "extra": [],
    "minimum-stability": "dev",
    "prefer-stable": true
}
