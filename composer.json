{
    "name": "silverstripe/auditor",
    "description": "Adds security audit trail to SilverStripe.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "audit"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.4 || ^8.0",
        "silverstripe/framework": "^4.12",
        "monolog/monolog": "~1.11",
        "psr/log": "~1.0",
        "tractorcow/silverstripe-proxy-db": "^1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.0",
        "silverstripe/versioned": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\Auditor\\": "code/",
            "SilverStripe\\Auditor\\Tests\\": "tests/"
        }
    },
    "extra": [],
    "minimum-stability": "dev",
    "prefer-stable": true
}